generator client {
    provider = "prisma-client"
    output   = "../../generated/prisma"
}

datasource db {
    provider = "postgresql"
}

model RsvpResponse {
    id        String   @id @default(cuid())
    firstName String
    lastName  String
    responses Json
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@unique([firstName, lastName])
}
