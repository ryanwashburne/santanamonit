{
	"name": "santanamonit",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"db:generate": "prisma migrate dev",
		"db:migrate": "prisma migrate deploy",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"dev": "next dev",
		"docker": "./scripts/via_docker.sh",
		"docker:start": "./scripts/docker_postgres.sh",
		"env": "vercel env pull .env",
		"postinstall": "prisma generate",
		"preview": "next build && next start",
		"setup": "vercel link --yes --project santanamonit && mv .env.local .env",
		"start": "next start",
		"typecheck": "tsc --noEmit",
		"vercel-build": "prisma generate && prisma migrate deploy && next build"
	},
	"dependencies": {
		"@chakra-ui/react": "^3.30.0",
		"@emotion/react": "^11.14.0",
		"@prisma/adapter-pg": "^7.2.0",
		"@prisma/client": "^7.2.0",
		"@react-email/render": "^2.0.2",
		"@sentry/nextjs": "^10.36.0",
		"@t3-oss/env-nextjs": "^0.12.0",
		"@tanstack/react-query": "^5.69.0",
		"@trpc/client": "^11.0.0",
		"@trpc/react-query": "^11.0.0",
		"@trpc/server": "^11.0.0",
		"@vercel/analytics": "^1.6.1",
		"motion": "^12.25.0",
		"next": "^16.1.1",
		"next-themes": "^0.4.6",
		"pg": "^8.16.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-icons": "^5.5.0",
		"resend": "^6.7.0",
		"server-only": "^0.0.1",
		"superjson": "^2.2.1",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@types/node": "^20.14.10",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"babel-plugin-react-compiler": "^1.0.0",
